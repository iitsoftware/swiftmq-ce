<swiftlet name="sys$scheduler"
					displayname="Scheduler Swiftlet"
					class="com.swiftmq.impl.scheduler.standard.SchedulerSwiftletImpl"
					icon="/sys_scheduler/images/swiftlet.png"
					description="Scheduler Swiftlet"
					release="${release}"
					vendor="IIT Software GmbH" >
	<configuration>
      <entitylist name="calendars"
                  display="Calendars"
                  description="Calendars"
                  icon="com/swiftmq/impl/scheduler/standard/images/calendar.png">
          <entitytemplate name="calendar"
                          display="Calendar"
                          description="Calendar"
                          icon="com/swiftmq/impl/scheduler/standard/images/calendar.png">
                <property name="type"
                          type="java.lang.String"
                          display="Calendar Type"
                          description="Calendar Type"
                          choice="include exclude"
                          default="exclude"/>
                <property name="base-calendar"
                          type="java.lang.String"
                          display="Base Calendar"
                          description="Calendar which is Base for this Calendar"/>
                <property name="enable-weekdays"
                          type="java.lang.Boolean"
                          display="Enable Week Days"
                          description="Enable Week Days on this Calendar"
                          default="false"/>
                <property name="enable-monthdays"
                          type="java.lang.Boolean"
                          display="Enable Month Days"
                          description="Enable Month Days on this Calendar"
                          default="false"/>
              <property name="enable-monthdays-last"
                        type="java.lang.Boolean"
                        display="Enable Last Month Day"
                        description="Enable Last Month Day on this Calendar"
                        default="false"/>
                <property name="enable-annualdays"
                          type="java.lang.Boolean"
                          display="Enable Annual Days"
                          description="Enable Annual Days on this Calendar"
                          default="false"/>
                <property name="enable-dateranges"
                          type="java.lang.Boolean"
                          display="Enable Dange Ranges"
                          description="Enable Dange Ranges on this Calendar"
                          default="false"/>
                <entity name="weekdays"
                        display="Week Days"
                        description="Week Days">
                      <property name="day-01"
                                type="java.lang.Boolean"
                                display="Sunday"
                                description="Sunday"
                                default="false" />
                      <property name="day-02"
                                type="java.lang.Boolean"
                                display="Monday"
                                description="Monday"
                                default="false" />
                      <property name="day-03"
                                type="java.lang.Boolean"
                                display="Tuesday"
                                description="Tuesday"
                                default="false" />
                      <property name="day-04"
                                type="java.lang.Boolean"
                                display="Wednesday"
                                description="Wednesday"
                                default="false" />
                      <property name="day-05"
                                type="java.lang.Boolean"
                                display="Thursday"
                                description="Thursday"
                                default="false" />
                      <property name="day-06"
                                type="java.lang.Boolean"
                                display="Friday"
                                description="Friday"
                                default="false" />
                      <property name="day-07"
                                type="java.lang.Boolean"
                                display="Saturday"
                                description="Saturday"
                                default="false" />
            </entity>
            <entity name="monthdays"
                    display="Month Days"
                    description="Month Days">
                  <property name="day-01"
                            type="java.lang.Boolean"
                            display="01."
                            description="01."
                            default="false" />
                  <property name="day-02"
                            type="java.lang.Boolean"
                            display="02."
                            description="02."
                            default="false" />
                  <property name="day-03"
                            type="java.lang.Boolean"
                            display="03."
                            description="03."
                            default="false" />
                  <property name="day-04"
                            type="java.lang.Boolean"
                            display="04."
                            description="04."
                            default="false" />
                  <property name="day-05"
                            type="java.lang.Boolean"
                            display="05."
                            description="05."
                            default="false" />
                  <property name="day-06"
                            type="java.lang.Boolean"
                            display="06."
                            description="06."
