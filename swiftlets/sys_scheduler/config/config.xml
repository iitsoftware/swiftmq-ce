                            default="false" />
                  <property name="day-19"
                            type="java.lang.Boolean"
                            display="19."
                            description="19."
                            default="false" />
                  <property name="day-20"
                            type="java.lang.Boolean"
                            display="20."
                            description="20."
                            default="false" />
                  <property name="day-21"
                            type="java.lang.Boolean"
                            display="21."
