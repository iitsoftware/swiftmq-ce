                            default="false" />
                  <property name="day-10"
                            type="java.lang.Boolean"
                            display="10."
                            description="10."
                            default="false" />
                  <property name="day-11"
                            type="java.lang.Boolean"
                            display="11."
                            description="11."
                            default="false" />
                  <property name="day-12"
                            type="java.lang.Boolean"
                            display="12."
                            description="12."
                            default="false" />
                  <property name="day-13"
                            type="java.lang.Boolean"
                            display="13."
                            description="13."
                            default="false" />
                  <property name="day-14"
                            type="java.lang.Boolean"
                            display="14."
                            description="14."
                            default="false" />
                  <property name="day-15"
                            type="java.lang.Boolean"
                            display="15."
                            description="15."
                            default="false" />
                  <property name="day-16"
                            type="java.lang.Boolean"
                            display="16."
                            description="16."
                            default="false" />
                  <property name="day-17"
                            type="java.lang.Boolean"
                            display="17."
